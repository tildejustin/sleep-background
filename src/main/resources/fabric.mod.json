{
  "schemaVersion": 1,
  "id": "sleepbackground",
  "version": "${version}",
  "name": "SleepBackground",
  "description": "Puts the instance to sleep by reducing framerate when Minecraft is running in the background.",
  "authors": [
    {
      "name": "RedLime",
      "contact": {
        "homepage": "https://github.com/RedLime/"
      }
    }
  ],
  "license": "MIT",
  "environment": "client",
  "custom": {
    "speedrunapi": {
      "config": "com.redlimerl.sleepbackground.SleepBackgroundConfig"
    }
  },
  "mixins": [
    "sleepbackground.mixins.json"
  ],
  "depends": {
    "minecraft": "1.16.1",
    "speedrunapi": "*"
  },
  "breaks": {
    "dynamicmenufps": "*"
  }
}
